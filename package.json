{
  "name": "winnie",
  "author": "Tyler Childs",
  "contributors": [
    "Herbert Spencer <herbert.spencer@gmail.com>"
  ],
  "license": "MIT",
  "description": "A Bootstrap compatible SASS-based framework for WOM",
  "homepage": "https://tinet-ux.github.io/winnie",
  "version": "1.3.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/tinet-ux/winnie.git"
  },
  "scripts": {
    "start": "gulp",
    "setup": "npm install"
  },
  "devDependencies": {
    "bower": "latest",
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-bump": "latest",
    "gulp-clean": "latest",
    "gulp-concat": "latest",
    "gulp-cssmin": "latest",
    "gulp-exec": "latest",
    "gulp-git": "latest",
    "gulp-include": "latest",
    "gulp-nunjucks-render": "^2.0.0",
    "gulp-rename": "latest",
    "gulp-replace": "latest",
    "gulp-sass": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-stylelint": "latest",
    "gulp-swig": "latest",
    "gulp-tag-version": "latest",
    "gulp-zip": "latest",
    "jquery": "latest",
    "kss": "^2.4.0",
    "minimist": "latest",
    "run-sequence": "latest"
  },
  "stylelint": {
    "rules": {
      "block-no-empty": null,
      "color-no-invalid-hex": true,
      "declaration-colon-space-after": "always",
      "indentation": [
        "tab",
        {
          "except": [
            "value"
          ]
        }
      ],
      "max-empty-lines": 2,
      "unit-whitelist": [
        "em",
        "rem",
        "%",
        "s"
      ]
    }
  }
}
